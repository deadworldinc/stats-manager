{
  "manifest_version": 3,
  "name": "Stats Manager",
  "version": "1.1",
  "description": "Manage stats",
  "permissions": ["cookies", "storage", "activeTab", "scripting", "webRequest"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/16.png",
      "48": "icons/48.png",
      "128": "icons/128.png"
    }
  }
}